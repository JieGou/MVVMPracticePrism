<UserControl x:Class="MVVMPracticePrism.Views.CustomRequest"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:hc="https://handyorg.github.io/handycontrol"
             xmlns:view="clr-namespace:MVVMPracticePrism.Views"
             xmlns:prism="http://prismlibrary.com/"             
             prism:ViewModelLocator.AutoWireViewModel="True">
    <StackPanel Margin="15,30,10,30">
        <Button Content="Open Dialog" Command="{Binding OpenCommand}"/>
        <TextBlock Text="{Binding Title}" FontSize="40"/>
    </StackPanel>
</UserControl>
